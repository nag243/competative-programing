// JS Quiz - 14
// Validation Trick in JS
const isRequired = () => {
    throw new Error("param is required")
}

const print = (value = isRequired()) => {
    console.log(`${value}`);
}

print(2);
// 2

print() //Throw Error
    // Uncaught Error: param is required


