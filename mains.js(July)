// JS Quiz - 17
// Count Occurrences of each Element in Array Using Reduce in JavaScript

const array = [1, 2, 3, 4, 1, 2, 2, 1]

array.reduce((acc, val) => {
    acc[val] = acc[val] ? acc[val] + 1 : 1
    return acc
}, {})

// output : {1: 3, 2: 3, 3: 1, 4: 1}

// JS Quiz - 18

// ð˜€ð˜ð—¿ð˜‚ð—°ð˜ð˜‚ð—¿ð—²ð—±ð—–ð—¹ð—¼ð—»ð—²() is the new way to create a deep clone of a given ð—¼ð—¯ð—·ð—²ð—°ð˜,
//  it uses the ð˜€ð˜ð—¿ð˜‚ð—°ð˜ð˜‚ð—¿ð—²ð—± ð—°ð—¹ð—¼ð—»ð—² ð—®ð—¹ð—´ð—¼ð—¿ð—¶ð˜ð—µð—º behind the scene.
const user1 = { name: 'AK', values: [1, 2] };

const user2 = structuredClone(user1);
user2.name = 'new name';
user2.values.push(3);

console.log(user1) // {name: 'AK' ;values :  [1, 2]}

console.log(user2) // {name: "new name" ;values:[1, 2, 3] }

